@model RoleValidateGuidViewModel
<h1>
    RoleValidationGuid
</h1>

<form asp-controller="SiteSetting" asp-action="RoleValidationGuid" method="post">
    <div asp-validation-summary="ModelOnly">    </div>
    <div class="from-group">
        <label class="control-label" asp-for="@Model.Value"></label>
        <input readonly="readonly" class="form-control"
               asp-for="@Model.Value" />
    </div>
    <div class="form-group">
        <label class="control-label" asp-for="@Model.LastChangeTime"></label>
        <input readonly="readonly" class="from-control"
               asp-for="@Model.LastChangeTime" />
    </div>
    <button class="btn btn-primary" type="submit">
        Generate Now Guid
    </button>
</form>
